# Run this once to generate a dummy model
from sklearn.ensemble import RandomForestClassifier
import pandas as pd
import pickle

df = pd.read_csv("data/expense_reports.csv")
df['Label'] = (df['Amount'] > 900).astype(int)

X = df[['Amount']]
y = df['Label']

model = RandomForestClassifier()
model.fit(X, y)

with open("models/expense_model.pkl", "wb") as f:
    pickle.dump(model, f)
